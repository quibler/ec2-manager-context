<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Code128 alphanumeric barcodes -->
        <record id="seq_product_barcode" model="ir.sequence">
            <field name="name">Product Auto Barcode</field>
            <field name="code">product.barcode.sequence</field>
            <field name="prefix">269-</field>     <!-- Matching your existing format -->
            <field name="suffix"> RW</field>      <!-- Adding the RW suffix like your existing codes -->
            <field name="padding">5</field>       <!-- 5 digits for the number part -->
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>